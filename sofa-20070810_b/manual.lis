     :               +1.30D-6,          +0.00D-6,
     :               +0.93D-6,          +0.00D-6 /
      DATA ( ( SS2(I,J), I=1,2), J= 11,20 ) /
     :               +0.68D-6,          +0.00D-6,
     :               -0.55D-6,          +0.00D-6,
     :               +0.53D-6,          +0.00D-6,
     :               -0.27D-6,          +0.00D-6,
     :               -0.27D-6,          +0.00D-6,
     :               -0.26D-6,          +0.00D-6,
     :               -0.25D-6,          +0.00D-6,
     :               +0.22D-6,          +0.00D-6,
     :               -0.21D-6,          +0.00D-6,
     :               +0.20D-6,          +0.00D-6 /
      DATA ( ( SS2(I,J), I=1,2), J= 21,NS2 ) /
