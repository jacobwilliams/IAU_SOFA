     :               -0.26D-6,          +0.00D-6 /
      DATA ( ( SS0(I,J), I=1,2), J= 21,30 ) /
     :               +0.21D-6,          +0.00D-6,
     :               -0.19D-6,          +0.00D-6,
     :               -0.18D-6,          +0.00D-6,
     :               +0.10D-6,          -0.05D-6,
     :               -0.15D-6,          +0.00D-6,
     :               +0.14D-6,          +0.00D-6,
     :               +0.14D-6,          +0.00D-6,
     :               -0.14D-6,          +0.00D-6,
     :               -0.14D-6,          +0.00D-6,
     :               -0.13D-6,          +0.00D-6 /
      DATA ( ( SS0(I,J), I=1,2), J= 31,NS0 ) /
     :               +0.11D-6,          +0.00D-6,
     :               -0.11D-6,          +0.00D-6,
     :               -0.11D-6,          +0.00D-6 /

*  Sine and cosine coefficients for t^1
      DATA ( ( SS1(I,J), I=1,2), J=  1,NS1 ) /
     :               -0.07D-6,          +3.57D-6,
     :               +1.71D-6,          -0.03D-6,
     :               +0.00D-6,          +0.48D-6 /

*  Sine and cosine coefficients for t^2
      DATA ( ( SS2(I,J), I=1,2), J=  1,10 ) /
     :             +743.53D-6,          -0.17D-6,
     :              +56.91D-6,          +0.06D-6,
     :               +9.84D-6,          -0.01D-6,
