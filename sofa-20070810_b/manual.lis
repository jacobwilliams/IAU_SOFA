     :              -11.21D-6,          -0.01D-6,
     :               +4.57D-6,          +0.00D-6,
     :               -2.02D-6,          +0.00D-6,
     :               -1.98D-6,          +0.00D-6,
     :               +1.72D-6,          +0.00D-6,
     :               +1.41D-6,          +0.01D-6,
     :               +1.26D-6,          +0.01D-6 /
      DATA ( ( SS0(I,J), I=1,2), J=11,20 ) /
     :               +0.63D-6,          +0.00D-6,
     :               +0.63D-6,          +0.00D-6,
     :               -0.46D-6,          +0.00D-6,
     :               -0.45D-6,          +0.00D-6,
